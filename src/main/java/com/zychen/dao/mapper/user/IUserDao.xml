<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zychen.dao.mapper.user.IUserDao">
    <insert id="addUser" parameterType="com.zychen.domain.User" useGeneratedKeys="true" keyProperty="groupId">
        INSERT INTO user(name, age)
        VALUES (#{name}, #{age});
    </insert>
</mapper>
